{
    "@context": "http://www.w3.org/ns/odrl.jsonld",
    "@type": "Policy",
    "uid": "https://upm.es/policy/21",
    "permission": [{
       "target": "https://jsonplaceholder.typicode.com/users/1",
       "action": "read",
       "constraint": [{
           "leftOperand": { "@value": "{{curre_time}}" , "@type": "xsd:time" },
           "operator": "eq",
           "rightOperand":  { "@value": "23:55:00", "@type": "xsd:time" }
       }]
   }]
}